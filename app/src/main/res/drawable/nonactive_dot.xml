<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item
    android:width="4dp"
    android:height="4dp"
    android:left="2dp"
    android:right="2dp">
    <rotate
        android:fromDegrees="45">
        <shape
            android:shape="rectangle">

            <size
                android:width="12dip"
                android:height="12dip" />


            <solid android:color="#C4C4C4"/>
        </shape>
    </rotate>
</item>
</layer-list>